<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<converter>
		<converter-for-class>ssms.entities.CustomerMstr</converter-for-class>
		<converter-class>ssms.entities.converter.CustomerMstrConverter</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>pc_Ssmslayout</managed-bean-name>
		<managed-bean-class>pagecode.layout.Ssmslayout</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_Report</managed-bean-name>
		<managed-bean-class>pagecode.Report</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_Import1</managed-bean-name>
		<managed-bean-class>pagecode.Import1</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>GoodsSpaceManager</managed-bean-name>
		<managed-bean-class>ssms.entities.controller.GoodsSpaceManager</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>CustomerMstrManager</managed-bean-name>
		<managed-bean-class>ssms.entities.controller.CustomerMstrManager</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_ReportResult</managed-bean-name>
		<managed-bean-class>pagecode.ReportResult</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_Export</managed-bean-name>
		<managed-bean-class>pagecode.Export</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_Contact</managed-bean-name>
		<managed-bean-class>pagecode.Contact</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_AddCustomer</managed-bean-name>
		<managed-bean-class>pagecode.AddCustomer</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_AddCustomerResult</managed-bean-name>
		<managed-bean-class>pagecode.AddCustomerResult</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/report.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>report</from-outcome>
			<to-view-id>/reportResult.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/import.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>import</from-outcome>
			<to-view-id>/importnotify.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/import.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/export.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>export</from-outcome>
			<to-view-id>/exportnotify.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/export.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/addCustomer.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>add</from-outcome>
			<to-view-id>/addCustomerResult.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/addCustomer.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
    <validator>
    	<validator-id>customerValidator</validator-id>
    	<validator-class>ssms.validation.CustomerMstrValidation</validator-class>
    </validator>
    <validator>
    	<validator-id>shipmentValidator</validator-id>
    	<validator-class>ssms.validation.ShipmentValidation</validator-class>
    </validator>
    <validator>
    	<validator-id>nouValidator</validator-id>
    	<validator-class>ssms.validation.NoOfUnitsValidation</validator-class>
    </validator>
</faces-config>